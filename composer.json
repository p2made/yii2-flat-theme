	"type": "yii2-extension",
	"keywords": [
		"yii2", "yii", "extension", "PHP",
		"asset", "bundle",
		"Flat", "theme",
		"frontend", "admin", "ui", "user-interface", "theme"
	],
	"license": "MIT",
	"authors": [
		{
			"name": "Pedro Plowman",
			"email": "p2opensource@gmail.com",
			"homepage": "https://github.com/p2made"
		}
	],
	"support": {
		"issues": "https://github.com/p2made/yii2-flat-theme/issues?state=open",
		"source": "https://github.com/p2made/yii2-flat-theme"
